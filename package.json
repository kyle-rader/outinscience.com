{
  "name": "wwupuzzlehunt",
  "version": "1.0.0",
  "description": "A Puzzle Hunt Game System",
  "main": "main.js",
  "scripts": {
    "start": "npm run meteor",
    "meteor": "meteor --settings settings-development.json || true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mup:set:dev": "cp mup-development.js mup.js && cp settings-development.json settings.json",
    "mup:set:prod": "cp mup-production.js mup.js && cp settings-production.json settings.json",
    "mup:deploy:dev": "mup deploy --config=mup-development.js --settings=settings-development.json",
    "mup:deploy:prod": "mup deploy --config=mup-production.js --settings=settings-production.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyle-rader/puzzlehunt.git"
  },
  "author": "Kyle W. Rader",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyle-rader/puzzlehunt/issues"
  },
  "homepage": "https://github.com/kyle-rader/puzzlehunt#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "meteor-node-stubs": "^0.2.3",
    "react": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-mounter": "^1.2.0",
    "react-scroll-up": "^1.1.5",
    "semantic-ui-react": "^0.54.5",
    "winston": "^2.2.0"
  }
}
